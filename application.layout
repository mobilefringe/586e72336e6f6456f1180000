    <script type="text/javascript">
        $(document).ready(function() {
            function renderPageData(){
                var pp = getPopups()[0];
                
                if (pp != undefined){
                    $('#pp_img').attr('src', 'https://mallmaverick.cdn.speedyrails.net' +pp.photo_url)
                    $('#pp_url').attr('href',  pp.photo_link)
                }
                //popup starts
                if($.cookie("popup_viewed") != "true"){
                    $.cookie("popup_viewed", "true");
                   $(".hidden-popup-bg").show();
                }
                
                $(".hidden-popup-bg").click(function(event){
                    if( !$( event.target).is('.hidden-popup-form') ) {
                        close_popup();
                    } else {
                        event.stopPropagation();
                    }
                });
                
                $(".hidden-popup-bg .hidden-popup-form").click(function(event){
                    event.stopPropagation();
                }); 
            }
            //loadMallData is located in mallmaverick.js, the call back method ensures the data is loaded
            loadMallData(renderPageData);
        });
        
    </script>
